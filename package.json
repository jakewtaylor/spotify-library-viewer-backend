{
  "name": "spotify-viewer-api",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/recoil": "^0.0.1",
    "axios": "^0.19.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts-laravel": "3.3.0-rc.3",
    "recoil": "^0.0.8",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "start": "HOST=spotify-viewer-api.test react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "mix": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": [
            "useRecoilCallback"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "http-proxy-middleware": "^1.0.4",
    "laravel-mix": "^5.0.4",
    "webpack-cli": "^3.3.11"
  }
}
